# Путь к весам YOLO
MODEL_PATH=./models/model.pt

# Разрешённые источники CORS (через запятую)
CORS_ORIGINS=http://localhost:5173

# # Путь к весам YOLO (если не задано, используется best.pt из корня)
# MODEL_PATH=best.pt

# # Разрешённые источники CORS (через запятую)
# CORS_ORIGINS=http://localhost:5173,http://localhost:5174

# # Регэксп для разрешённых доменов (например, все проекты на Vercel)
# CORS_ORIGIN_REGEX=https://.*\.vercel\.app

# --- Roboflow Hosted Inference (optional) ---
# Enable signature detection via Roboflow Hosted Inference by setting model id and API key.
# Example model id from Roboflow Universe: "signature-krkm0/1"
# ROBOFLOW_MODEL_ID=signature-krkm0/1
# ROBOFLOW_API_KEY=your_api_key_here
# Optional custom endpoint (otherwise built from model id):
# SIGNATURE_ROBOFLOW_ENDPOINT=https://detect.roboflow.com/signature-krkm0/1?api_key=your_api_key_here&format=json

# Optional tuning for Roboflow inference (defaults shown)
# Lower confidence can yield more detections at the cost of false positives.
# ROBOFLOW_CONF=0.25
# ROBOFLOW_OVERLAP=30
# ROBOFLOW_FORMAT=json